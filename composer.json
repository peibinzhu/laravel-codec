{
  "name": "peibin/laravel-codec",
  "description": "Peibin codec component for Laravel.",
  "license": "MIT",
  "keywords": [
    "php",
    "laravel"
  ],
  "authors": [
    {
      "name": "Peibin Zhu",
      "email": "zhupeibin1207@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "ext-json": "*",
    "ext-xml": "*",
    "peibin/laravel-contracts": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "PeibinLaravel\\Codec\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PeibinLaravelTest\\Codec\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "prefer-stable": true
}
